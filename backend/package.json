{
    "name": "backend",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "prepare": "exit 0",
        "clean": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write '{src,test}/**/*.ts'",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:prod": "node dist/src/main",
        "lint": "eslint",
        "lint:staged": "lint-staged",
        "lint:fix": "eslint --fix",
        "test": "TZ=UTC jest --config test/jest.json --runInBand --passWithNoTests --forceExit",
        "deadcode": "ts-prune --project tsconfig.json --unusedInModule --skip *.json",
        "spell": "cspell lint --config cspell.json '{src,test}/**/*.ts' --color --gitignore --no-must-find-files --no-summary --no-progress || true",
        "upgrade:package": "ncu -u",
        "seed": "nestjs-command seed:email && nestjs-command seed:setting  && nestjs-command seed:apikey && nestjs-command seed:role && nestjs-command seed:user && nestjs-command seed:template",
        "rollback": "nestjs-command remove:email && nestjs-command remove:setting && nestjs-command remove:apikey && nestjs-command remove:user && nestjs-command remove:role && nestjs-command remove:template",
        "sentry:sourcemaps": "sentry-cli sourcemaps inject --org form-builder --project node-nestjs ./ && sentry-cli sourcemaps upload --org form-builder --project node-nestjs ./"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.583.0",
